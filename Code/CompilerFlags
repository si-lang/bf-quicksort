Sequential:
g++-7 -std=c++14 -pthread -lm -lc -lgcc_s -lgcc -O3 -march=native PartitionAndSwapTest.cpp -o partitionAndSwapTest
g++-7 -std=c++14 -pthread -lm -lc -lgcc_s -lgcc -O3 -march=native QuickSortTest.cpp -o quickSortTest

g++-7 -std=c++14 -pthread -lm -lc -lgcc_s -lgcc -O3 -march=native PartitionProfile.cpp -o partitionProfile -L../TimeAndProfile -ljevents -ltbb
g++-7 -std=c++14 -pthread -lm -lc -lgcc_s -lgcc -O3 -march=native QuickSortProfile.cpp -o quickSortProfile -ltbb


