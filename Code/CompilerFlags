Parallel (GNU Parallel Mode: -D_GLIBCXX_PARALLEL -fopenmp):
g++-7 -std=c++14 -pthread -lm -lc -lgcc_s -lgcc -O3 -march=native ParallelPartitionAndSwapTest.cpp -o parallelPartitionAndSwapTest
g++-7 -std=c++14 -pthread -lm -lc -lgcc_s -lgcc -O3 -march=native ParallelQuickSortTest.cpp -o parallelQuickSortTest

g++-7 -std=c++14 -pthread -lm -lc -lgcc_s -lgcc -O3 -march=native ParallelPartitionProfile.cpp -o parallelPartitionProfile -L../TimeAndProfile -ljevents -ltbb
g++-7 -std=c++14 -pthread -lm -lc -lgcc_s -lgcc -O3 -march=native -fopenmp ParallelPartitionProfile.cpp -o parallelPartitionProfile -L../TimeAndProfile -ljevents -ltbb -D_GLIBCXX_PARALLEL
g++-7 -std=c++14 -pthread -lm -lc -lgcc_s -lgcc -O3 -march=native ParallelQuickSortProfile.cpp -o parallelQuickSortProfile -L../TimeAndProfile -ljevents -ltbb
g++-6 -std=c++14 -pthread -lm -lc -lgcc_s -lgcc -O3 -march=native -D_GLIBCXX_PARALLEL -fopenmp ParallelQuickSortProfile.cpp -o parallelQuickSortProfile -L../TimeAndProfile -ljevents -ltbb

g++-7 -std=c++14 -pthread -lm -lc -lgcc_s -lgcc -O3 -march=native simpleExec.cpp -o simpleExec -L../TimeAndProfile -ljevents -ltbb


